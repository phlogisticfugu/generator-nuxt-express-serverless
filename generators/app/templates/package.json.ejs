{
  "name": "<%= appname %>",
  "version": "0.0.1",
  "description": "<%= appdescription %>",
  "private": true,
  "scripts": {
    "rollup": "cross-env NODE_ENV=development rollup -c",
    "serve-dev": "cross-env NODE_ENV=development node server_local.js",
    "dev": "npm run rollup && npm run serve-dev",
    "build-prod": "nuxt build && cross-env NODE_ENV=production rollup -c",
    "deploy-staging": "npm run build-prod && serverless deploy --stage staging",
    "deploy-prod": "npm run build-prod && serverless deploy --stage prod",
    "logs-staging": "serverless logs --function express --stage staging -t",
    "logs-prod": "serverless logs --function express --stage prod -t",
    "create-domain-staging": "serverless --stage=staging create_domain",
    "create-domain-prod": "serverless --stage=prod create_domain",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "engines": {
    "node": ">=8.10"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "aws-serverless-express": "^3.3.5",
    "bunyan": "^1.8.12",
    "express": "^4.16.3",
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "serverless": "^1.33.2",
    "serverless-domain-manager": "^2.6.6"
  }
}
